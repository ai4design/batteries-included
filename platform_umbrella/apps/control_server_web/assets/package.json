{
  "license": "COMMERCIAL",
  "name": "control_server_web_assets",
  "scripts": {
    "css:deploy": "NODE_ENV=production tailwindcss --postcss --minify --input=css/app.css --output=dist/app.css",
    "css:deploy:dev": "tailwindcss --postcss --input=css/app.css --output=../priv/static/assets/app.css",
    "js:deploy": "NODE_ENV=production esbuild js/app.ts --external:/fonts/* --external:/images/* --bundle --target=chrome109,firefox112,safari15.6,edge112 --outdir=dist",
    "js:deploy:dev": "NODE_ENV=production esbuild js/app.ts --external:/fonts/* --external:/images/* --bundle --target=chrome109,firefox112,safari15.6,edge112 --outdir=../priv/static/assets"
  },
  "dependencies": {
    "@fontsource-variable/inter": "5.0.13",
    "@fontsource-variable/jetbrains-mono": "5.0.16",
    "alpinejs": "3.13.2",
    "chart.js": "4.4.0",
    "esbuild": "0.19.5",
    "lodash": "4.17.21",
    "phoenix": "1.7.9",
    "phoenix_html": "3.3.3",
    "phoenix_live_view": "0.20.1",
    "tippy.js": "6.3.7",
    "topbar": "2.0.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.6",
    "@tailwindcss/typography": "0.5.10",
    "@types/alpinejs": "^3.13.3",
    "@types/phoenix": "^1.6.2",
    "@types/phoenix_live_view": "^0.18.2",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "daisyui": "3.9.3",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-url": "^10.1.3",
    "tailwindcss": "3.3.3"
  }
}
