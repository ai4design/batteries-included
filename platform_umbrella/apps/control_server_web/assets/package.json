    "tippy.js": "6.3.7",
    "topbar": "2.0.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.4",
    "@tailwindcss/typography": "0.5.9",
    "@types/alpinejs": "^3.7.2",
    "@types/phoenix": "^1.6.0",
