{
  "scripts": {
    "tailwind": "NODE_PATH=./node_modules tailwindcss --postcss",
    "esbuild": "NODE_PATH=./node_modules:../../../deps esbuild --bundle --target=chrome109,firefox112,safari15.6,edge112",
    "css:deploy": "NODE_ENV=production npm run tailwind -- --input=css/app.css --output=../priv/static/assets/app.css --minify",
    "css:deploy:dev": "npm run tailwind -- --input=css/app.css --output=../priv/static/assets/app.css",
    "js:deploy": "NODE_ENV=production npm run esbuild -- js/app.js --outdir=../priv/static/assets --minify --analyze",
    "js:deploy:dev": "npm run esbuild -- js/app.js --outdir=../priv/static/assets"
  },
  "dependencies": {
    "@fontsource-variable/inter": "~5.0",
    "@fontsource-variable/jetbrains-mono": "~5.0",
    "@tailwindcss/forms": "~0.5",
    "@tailwindcss/typography": "~0.5",
    "alpinejs": "~3.13",
    "autoprefixer": "~10.4",
    "chart.js": "~4.4",
    "esbuild": "~0.20",
    "lodash": "~4.17",
    "postcss": "~8.4",
    "postcss-import": "~16.0",
    "postcss-url": "~10.1",
    "tailwindcss": "~3.4",
    "tippy.js": "~6.3",
    "topbar": "~2.0"
  }
}
